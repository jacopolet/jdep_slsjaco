    <fields>
        <fullName>Exclude_Weekends__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( 
MOD( Sent_for_Approval_Date__c - DATE( 1900, 1, 7 ), 7 ), 
3, Sent_for_Approval_Date__c + 2 + 3, 
4, Sent_for_Approval_Date__c + 2 + 3, 
5, Sent_for_Approval_Date__c + 2 + 3, 
6, Sent_for_Approval_Date__c + 1 + 3, 
Sent_for_Approval_Date__c + 3 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Exclude Weekends</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
