        <lookupDialogsAdditionalFields>Rebate_Payout__c</lookupDialogsAdditionalFields>
        <searchResultsAdditionalFields>Bank_Account__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>IBAN__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Bank_Country__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Bank_Key__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Collection_authorization__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Rebate_Payout__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Bank_account_and_Bank_Key</fullName>
        <active>true</active>
        <description>Bank Key and bank account are dependent to each other</description>
        <errorConditionFormula>AND(
		$Setup.ValidationRulesSettings__c.BankDetailValidationRule__c, ISNEW(), 
		OR(
			AND(ISBLANK(Approval_Bank_Account__c), 
			   OR(NOT(ISBLANK( Approval_Bank_Key__c )) || NOT(ISBLANK( Bank_Key__c)))
			), 
			AND(ISBLANK(Approval_Bank_Key__c), 
			   OR(NOT(ISBLANK(Bank_Account__c)), NOT(ISBLANK( Approval_Bank_Account__c)))
			)
		)
	)</errorConditionFormula>
        <errorMessage>Please fill in both Bank Account and Bank Key</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Creation_Not_Allowed_on_Prospect</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $Setup.ValidationRulesSettings__c.BankDetailValidationRule__c, 
 ISNEW(),
  ISBLANK(Account__r.SAP_Customer_ID__c)
)</errorConditionFormula>
        <errorMessage>You can only create bank details on Accounts which have a valid SAP Customer ID. Please try again after promoting the Account to SAP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IBAN</fullName>
        <active>true</active>
        <description>IBAN is mandatory if bank key and bank account not filled in</description>
        <errorConditionFormula>AND( 
$Setup.ValidationRulesSettings__c.BankDetailValidationRule__c, ISNEW(), ISBLANK(Approval_IBAN__c), ISBLANK(Approval_Bank_Account__c), ISBLANK(Approval_Bank_Key__c)
)</errorConditionFormula>
        <errorMessage>Please fill in IBAN or Bank Key and Bank Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pending_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.BankDetailValidationRule__c, 
OR( AND(NOT(ISBLANK( PRIORVALUE(Approval_Bank_Account__c))), ISCHANGED(Approval_Bank_Account__c)), 
    AND(NOT(ISBLANK( PRIORVALUE(Approval_Bank_Country__c))),ISCHANGED(Approval_Bank_Country__c)), 
    AND(NOT(ISBLANK( PRIORVALUE(Approval_Bank_Key__c))),ISCHANGED(Approval_Bank_Key__c)), 
    AND(PRIORVALUE(Approval_Collection_Authorization__c) == true, Approval_Collection_Authorization__c= false), 
    AND(NOT(ISBLANK( PRIORVALUE(Approval_Control_Key__c))),ISCHANGED(Approval_Control_Key__c)),
    AND(NOT(ISBLANK( PRIORVALUE(Approval_IBAN__c))),ISCHANGED(Approval_IBAN__c))
), 
ISPICKVAL(Updated_Request_Validation__c , &apos;Approval Pending&apos;))</errorConditionFormula>
        <errorMessage>An Approval is already in progress. Please do not request for a change. If needed recall existing approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SapValidationInProgress</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $Setup.ValidationRulesSettings__c.AccountValidationRules__c, $Profile.Name &lt;&gt; $Label.JDE_Integ_Sap, 
 ISPICKVAL(Updated_Request_Validation__c, &quot;Approval Validated&quot;),
OR(
NOT(ISBLANK(Updated_Bank_Account__c)),
NOT(ISPICKVAL(Updated_Bank_Country__c, &quot;&quot;)),
NOT(ISBLANK(Updated_Control_Key__c)),
NOT(ISBLANK(Updated_Bank_Key__c))
)
)</errorConditionFormula>
        <errorMessage>There is currently a SAP validation in progress!</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
