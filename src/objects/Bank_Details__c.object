        <externalId>false</externalId>
        <formula>CASESAFEID(Account__r.Id) &amp; 
IF(ISBLANK(TEXT(Updated_Bank_Country__c)), TEXT(Bank_Country__c), TEXT(Updated_Bank_Country__c)) &amp; 
IF(ISBLANK(Updated_Bank_Key__c),  Bank_Key__r.Bank_Number__c , Updated_Bank_Key__r.Bank_Number__c ) &amp; 
IF(ISBLANK(Updated_Bank_Account__c), Bank_Account__c, Updated_Bank_Account__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
