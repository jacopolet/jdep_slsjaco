    <validationRules>
        <fullName>Cant_CloseMileston_FS_Frst_Case_manually</fullName>
        <active>true</active>
        <description>When an Work ORder is still open you cannot close the case manually</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.CaseValidationRules__c = TRUE, NOT(ISBLANK(CompletionDate))&amp;&amp;
ISPICKVAL(Case.Type,&quot;Field Service&quot;) &amp;&amp;
Case.TS_FirstRepsonseMilestoneautoclosed__c = FALSE &amp;&amp;
NOT(AND( ISPICKVAL(Case.CompletionCode__c, &apos;140&apos;), NOT( ISBLANK( Case.External_Solution__c ) ))),FALSE)</errorConditionFormula>
        <errorMessage>You cannot close the First Response Milestone manually as the related Work Order is still open.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Close_Qual_milestone_Manually</fullName>
        <active>true</active>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.CaseValidationRules__c = TRUE &amp;&amp;
ISBLANK(Case.CaseQualificationDate__c)&amp;&amp;
NOT(ISBLANK(CompletionDate))&amp;&amp;
ISPICKVAL(Case.Origin ,&quot;E-mail&quot;)</errorConditionFormula>
        <errorMessage>You cannot close this milestone manually, Qualify the case instead using the Case Qualify button</errorMessage>
    </validationRules>
