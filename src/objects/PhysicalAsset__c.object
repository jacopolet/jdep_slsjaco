    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>TS_CantCreateCRFUPInactive</fullName>
        <active>true</active>
        <description>You cannot create Counter Reading FUP if machine is inactive.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c  &amp;&amp; 
CreateS22__c &amp;&amp; 
ISCHANGED( CreateS22__c ) &amp;&amp; 
NOT(ISPICKVAL(AssetStatus__c, &apos;Activated&apos;))</errorConditionFormula>
        <errorMessage>You cannot create Counter Reading FUP if machine is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CantCreateCRFUPNotRequired</fullName>
        <active>true</active>
        <description>You cannot create Counter Reading FUP if counter is not required in the machine.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c  &amp;&amp; 
CreateS22__c &amp;&amp; 
ISCHANGED( CreateS22__c ) &amp;&amp; 
ISPICKVAL(CounterReading__c, &apos;No&apos;)</errorConditionFormula>
        <errorMessage>You cannot create Counter Reading FUP if counter is not required in the machine.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowAccountLocationChanges</fullName>
        <active>true</active>
        <description>You are not allowed to update the account location.</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c,
ISCHANGED(AccountLocation__c) &amp;&amp; ISCHANGED(QuickActionUpdateCounter__c) &amp;&amp; NOT($Permission.TS_UpdateAccountLocationOnPA),
FALSE)</errorConditionFormula>
        <errorMessage>You are not allowed to update the account location.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowCreateCorrCounterReadings</fullName>
        <active>true</active>
        <description>You are not allowed to create a corrective counter reading job.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; 
ISCHANGED( CreateS22__c  ) &amp;&amp;
NOT( $Permission.TS_S22  )</errorConditionFormula>
        <errorMessage>You are not allowed to create a corrective counter reading job.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowInvalidateCounters</fullName>
        <active>true</active>
        <description>You are not allowed to invalidate counter readings.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; ISCHANGED(  InvalidateCounters__c  ) &amp;&amp;
NOT( $Permission.TS_InvalidateCounterReadings )</errorConditionFormula>
        <errorMessage>You are not allowed to invalidate counter readings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowLocationDetailChanges</fullName>
        <active>false</active>
        <description>You are not allowed to update the location details.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; 
OR(
ISCHANGED( Building__c ),
ISCHANGED( Floor__c ),
ISCHANGED( Area__c ),
ISCHANGED( CustomerReferenceNumber__c ),
ISCHANGED( SmokingArea__c )) &amp;&amp;
NOT( $Permission.TS_UpdateLocationOnPA )</errorConditionFormula>
        <errorMessage>You are not allowed to update the location details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowMothballIndicatorChange</fullName>
        <active>true</active>
        <description>You are not allowed to update the mothball indicator.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; ISCHANGED(   MothballAssetIndicator__c   ) &amp;&amp;
NOT(  $Permission.TS_IndicateMothball  )</errorConditionFormula>
        <errorMessage>You are not allowed to update the mothball indicator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowOverridePrevMaintenaceDate</fullName>
        <active>false</active>
        <description>You are not allowed to override the next preventive mainteanance date.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; ISCHANGED( NextWTSDate__c ) &amp;&amp;
NOT($Permission.TS_OverrideNextPrevMainDateonPA)</errorConditionFormula>
        <errorDisplayField>AccountLocation__c</errorDisplayField>
        <errorMessage>You are not allowed to override the next water filter change date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowOverrideWTSDate</fullName>
        <active>false</active>
        <description>You are not allowed to override the next water filter change date.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; ISCHANGED( NextWTSDate__c ) &amp;&amp;
NOT( $Permission.TS_OverrideNextWTSDateonPA )</errorConditionFormula>
        <errorDisplayField>AccountLocation__c</errorDisplayField>
        <errorMessage>You are not allowed to override the next water filter change date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowSerialNumberChange</fullName>
        <active>true</active>
        <description>You are not allowed to update the serial number.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; ISCHANGED( SerialNumber__c ) &amp;&amp;
NOT( $Permission.TS_UpdateSerialNumberOnPA )</errorConditionFormula>
        <errorMessage>You are not allowed to update the serial number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowWTSIntervalUpdate</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c,
AND($Permission.TS_Update_WTS_Interval = FALSE,ISCHANGED(WTSInterval__c)),
FALSE
)</errorConditionFormula>
        <errorMessage>You are not allowed to update WTS Interval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NotAllowedToCreateAssetManually</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Ismanuallycreated__c) &amp;&amp; 
ISNEW() &amp;&amp; 
!$Permission.TS_AllowPACreation</errorConditionFormula>
        <errorMessage>You are not permitted to create a machine/option manually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_PreventCorrectCounterMothball</fullName>
        <active>true</active>
        <description>You cannot create a corrective counter reading job for a mothball indicated machine.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.PhysicalAssetValidationRules__c &amp;&amp; 
ISCHANGED( CreateS22__c  ) &amp;&amp;
CreateS22__c  = TRUE &amp;&amp;
MothballAssetIndicator__c = TRUE</errorConditionFormula>
        <errorMessage>You cannot create a corrective counter reading job for a mothball indicated machine.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_UnableToUpdateNextPreventiveMain</fullName>
        <active>true</active>
        <description>[KTS-392]: Contract Triggered Cases (Preventive Maintenance) - Rule to prevent from updating next preventive maintenance date when preventive maintenance planned is true</description>
        <errorConditionFormula>IF((ISCHANGED(NextPreventiveMaintenanceDate__c) &amp;&amp;  PreventiveMaintenancePlanned__c &amp;&amp;  PreventiveMaintenanceManualUpdate__c), true, false)</errorConditionFormula>
        <errorMessage>Next Preventive Maintenance Date cannot be change when Preventive Maintenance Planned is already set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_UnableToUpdateNextWTS</fullName>
        <active>true</active>
        <description>[KTS-1095]: Contract Triggered Cases (Waterfilter Changes) - Rule to prevent from updating next WTS date when WTS planned is true</description>
        <errorConditionFormula>IF((ISCHANGED(NextWTSDate__c) &amp;&amp; WTSPlanned__c &amp;&amp; WTSManualUpdate__c), true, false)</errorConditionFormula>
        <errorMessage>Next WTS Date cannot be change when WTS Planned is already set</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
