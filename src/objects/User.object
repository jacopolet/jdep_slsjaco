    <validationRules>
        <fullName>DevOps_Profile_Validation</fullName>
        <active>true</active>
        <description>Users with DevOps profile will not be able to edit user details.</description>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.UsersValidationRule__c = true , $Profile.Name  = &quot;JDE - DevOps Profile&quot; )</errorConditionFormula>
        <errorDisplayField>EmailPreferences</errorDisplayField>
        <errorMessage>Editing user details is restricted for JDE-DevOps Profile</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Elevated_Access_Reason</fullName>
        <active>false</active>
        <description>Verifies that a reason is provided when requesting elevated access (SYSTEM ADMINISTRATOR PROFILE)</description>
        <errorConditionFormula>AND( CONTAINS( UPPER(Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;),ISPICKVAL( Elevated_Access_Reason__c,&quot;&quot;), NOT(Privileged_User__c))</errorConditionFormula>
        <errorMessage>Please provide a reason for requesting elevated access</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Elevated_Access_Reference</fullName>
        <active>false</active>
        <description>Verifies that a reference is provided when requesting elevated access (SYSTEM ADMINISTRATOR PROFILE)</description>
        <errorConditionFormula>AND( CONTAINS(UPPER(Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;),ISBLANK( Elevated_Access_Reference__c),NOT(Privileged_User__c))</errorConditionFormula>
        <errorMessage>Please provide a reference (defect, incident, task) for requesting elevated access</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Privileged_Access_Manager</fullName>
        <active>false</active>
        <description>Verifies that only users with the custom permission UAM_Privileged_Access_Manager can assign the privileged access on a permanent basis</description>
        <errorConditionFormula>AND( NOT($Permission.UAM_Privileged_Access_Manager), Privileged_User__c)</errorConditionFormula>
        <errorMessage>Only Privileged Access Managers can set the Privileged User checkbox to true</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_System_Admin</fullName>
        <active>false</active>
        <description>Prevents the assignment of the SYSTEM ADMINISTRATOR profile during user creation. This is to prevent that there is no original profile to revert back to after the temporary assignment</description>
        <errorConditionFormula>AND(CONTAINS(UPPER(Profile.Name), &quot;SYSTEM ADMINISTRATOR&quot;),ISNEW(),NOT(Privileged_User__c))</errorConditionFormula>
        <errorMessage>You cannot assign the System Administrator profile during user creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_edit_for_end_users</fullName>
        <active>true</active>
        <description>first, middle or last name, alias, email or nick name should not be editable for end users. It could only be edited by system administrators, functional testers and on creation</description>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.UsersValidationRule__c,
OR(
ISCHANGED( FirstName ),
ISCHANGED( MiddleName ),
ISCHANGED( LastName ),
ISCHANGED( Alias ),
ISCHANGED( Email ),
ISCHANGED( CommunityNickname )
),
$Profile.Name != &apos;System Administrator&apos;, $Profile.Name != &apos;JDE-Admin-Functional Tester&apos;, $Profile.Name != &apos;JDE-Admin-User Administrator&apos;,
NOT(ISNEW())
)</errorConditionFormula>
        <errorMessage>It is not allowed to change the first name, middle name, last name, alias, email or nick name. Please contact a system administrator in case you require a change of any of these fields.</errorMessage>
    </validationRules>
