        <fullName>Data_Quality_Description__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Data_Quality_Score__c == 100, &apos;All Competitor Services Captured&apos;, &apos;Missing: &apos; &amp;BR() 
&amp;IF (ISBLANK(Product__c),&apos;Product &apos; &amp;BR(),&apos;&apos;) 
&amp;IF (ISBLANK(Supplier__c), &apos;Supplier &apos; &amp;BR() , &apos;&apos;) 
&amp;IF (ISBLANK(TEXT(Supplier_Type__c)),&apos;Supplier Type&apos; &amp;BR() ,&apos;&apos;) 
&amp;IF (ISBLANK(Expiration_date_Contract__c), &apos;Expiration Date Contract&apos; &amp;BR() , &apos;&apos;) 
