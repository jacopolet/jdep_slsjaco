        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK (Product__c)), 25, 0) + IF(NOT(ISBLANK (TEXT(Supplier_Type__c))), 25, 0) + IF(NOT(ISBLANK (Supplier__c)), 25, 0) + IF(NOT(ISBLANK (Expiration_date_Contract__c)), 25, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Data Quality Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
