    <validationRules>
        <fullName>Value_change_Check</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL(APTS_Index_Type__c, &apos;Non-Standard Indexation&apos;),
      NOT(
         OR(
         ISCHANGED(APTS_Additional_Conditions__c),
         ISCHANGED(APTS_Communication_Deadline__c),
         ISCHANGED(APTS_Communication_Needed__c),
         ISCHANGED(APTS_Customer_Approval_Required__c),
         ISCHANGED(APTS_Frequency__c),
         ISCHANGED(APTS_Indexation_Category__c),
         ISCHANGED(APTS_Indexation_Number__c),
         ISCHANGED(APTS_Maximum_Bandwidth__c),
         ISCHANGED(APTS_Start_Date__c)
        )
      )
)</errorConditionFormula>
        <errorMessage>You have defined an indexation without changing any Indexation specific values. Please change back to “Standard Indexation” or change any Indexation specific values.</errorMessage>
    </validationRules>
