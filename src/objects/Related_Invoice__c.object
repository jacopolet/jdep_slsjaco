    <validationRules>
        <fullName>TS_RelatedInvoiceRules</fullName>
        <active>false</active>
        <description>Only the owner of the case can relate invoices to the case and only before the case is closed or canceled.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.CaseValidationRules__c &amp;&amp; 


OR(
ISNEW() &amp;&amp; Case__r.OwnerId &lt;&gt; LastModifiedById, 
NOT(ISNEW()) &amp;&amp; Case__r.OwnerId &lt;&gt; LastModifiedById,
ISNEW() &amp;&amp; OR(ISPICKVAL(Case__r.Status, &quot;Canceled&quot;), ISPICKVAL(Case__r.Status, &quot;Closed&quot;)))</errorConditionFormula>
        <errorMessage>Only the owner of the case can relate invoices to the case and only before the case is closed or canceled.</errorMessage>
    </validationRules>
