    <validationRules>
        <fullName>TS_CannotIndicate247ForComfortOperating</fullName>
        <active>true</active>
        <description>You cannot indicate a 24/7 schedule for an asset with comfort operating support only.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c &amp;&amp;
OP_247Customer__c = TRUE &amp;&amp;
ISPICKVAL(  PhysicalAsset__r.Machine_Care__c , &quot;Comfort Operating&quot;)</errorConditionFormula>
        <errorMessage>You cannot indicate a 24/7 schedule for an asset with comfort operating support only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CreateScheduleForCanceledMachine</fullName>
        <active>true</active>
        <description>You cannot create a schedule for a canceled machine.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c &amp;&amp;
ISNEW() &amp;&amp;
ISPICKVAL( PhysicalAsset__r.AssetStatus__c , &quot;Canceled&quot;)</errorConditionFormula>
        <errorMessage>You cannot create a schedule for a canceled machine.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_DeclineForceLToNextBusDay</fullName>
        <active>true</active>
        <description>You cannot fora a L to the next business day (holiday) if the schedule indicates to schedule jobs on holidays.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c &amp;&amp;
ForceScheduleToNextBusinessDay__c = TRUE &amp;&amp;
Schedule_Tasks_for_Holiday__c = TRUE</errorConditionFormula>
        <errorMessage>You cannot fora a L to the next business day (holiday) if the schedule indicates to schedule jobs on holidays.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NoSupportForFullCoOrSemi</fullName>
        <active>true</active>
        <description>You cannot create/activate an operating schedule for full co operating or semi operating machines or those with no machine care.</description>
        <errorConditionFormula>/*Karen Hung KTS-2694 ALM- 6754 25-04-2019*/
IF($Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c,
	AND(
	    OR(ISPICKVAL(PhysicalAsset__r.Machine_Care__c , &apos;Full Co Operating&apos;),
		    ISPICKVAL(PhysicalAsset__r.Machine_Care__c , &apos;Semi Operating&apos;),
		    ISPICKVAL(PhysicalAsset__r.Machine_Care__c , &apos;&apos;)
	    ),
	    OR(
		    ISNEW(),
		    Active__c
	    )
	),
	FALSE
)</errorConditionFormula>
        <errorMessage>You cannot create/activate an operating schedule for full co operating or semi operating machines or those with no machine care.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_Op_InitReDatemustbeSunday</fullName>
        <active>true</active>
        <description>Initial_Re_activation_Date__c must be a Sunday</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c &amp;&amp; 
MOD(   Start_Date__c  - DATE(1900, 1, 6), 7) &lt;&gt; 1 &amp;&amp;
ISPICKVAL( Schedule_Type__c , &quot;Weekly&quot;)</errorConditionFormula>
        <errorMessage>For weekly schedules the startdate can only be on a Sunday as it indicates the first rundate of the batch job to be executed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_Op_StartDatemustbeMonday</fullName>
        <active>true</active>
        <description>When the date entered is not a Monday the validation rule is preventing the save of the record. this is used for ensuring Batch job can generate Operating Tasks properly.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c &amp;&amp;
(MOD(  Start_Date__c  - DATE(1900, 1, 6), 7) &lt;&gt; 2 &amp;&amp;  ISPICKVAL(Schedule_Type__c , &quot;Regular&quot;))</errorConditionFormula>
        <errorMessage>The start date for regular schedule must be on a Monday.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_UpdateCommentWhenEdited</fullName>
        <active>true</active>
        <description>To require comment when operating schedule is being updated by the user</description>
        <errorConditionFormula>/*Karen Hung Jan 11, 2021 DOO-5040*/
IF($Setup.ValidationRulesSettings__c.OperatingScheduleValidationRules__c,
NOT(ISNEW()) &amp;&amp;
$User.LastName != &apos;JDE-Apex-Batch&apos; &amp;&amp; 
OR(NOT(ISCHANGED(Comment__c)) , ISBLANK(Comment__c)),
false
)</errorConditionFormula>
        <errorMessage>Please update the Comment field to indicate the reason why the schedule is edited.</errorMessage>
    </validationRules>
