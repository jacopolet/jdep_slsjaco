RecordType.Name = &quot;Daily&quot; &amp;&amp;
OR(
ISPICKVAL( LM_1_Monday__c,&quot;&quot;),
ISPICKVAL( LM_1_Tuesday__c ,&quot;&quot;),
ISPICKVAL( LM_1_Wednesday__c ,&quot;&quot;),
ISPICKVAL( LM_1_Thursday__c ,&quot;&quot;),
ISPICKVAL( LM_1_Friday__c ,&quot;&quot;),
