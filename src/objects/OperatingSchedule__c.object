ISPICKVAL( LM_2_Sunday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Monday__c,&quot;&quot;),
ISPICKVAL( SM_1_Tuesday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Wednesday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Thursday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Friday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Saturday__c ,&quot;&quot;),
ISPICKVAL( SM_1_Sunday__c ,&quot;&quot;),
