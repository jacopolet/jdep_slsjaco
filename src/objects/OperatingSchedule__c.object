    <fields>
        <fullName>Weekly_Batch_Count__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
	Specific_Days_in_Week__c = TRUE,
	(
		IF(
			NOT(ISBLANK(TEXT(SM_1_Monday__c))),
			IF(ISPICKVAL(SM_1_Monday__c, &quot;AM&quot;), 1, VALUE(TEXT(SM_1_Monday__c))),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Tuesday__c))),
			VALUE(TEXT(SM_1_Tuesday__c)),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Wednesday__c))),
			VALUE(TEXT(SM_1_Wednesday__c)),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Thursday__c))),
			VALUE(TEXT(SM_1_Thursday__c)),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Friday__c))),
			IF(ISPICKVAL(SM_1_Friday__c, &quot;PM&quot;), 1, VALUE(TEXT(SM_1_Friday__c))),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Saturday__c))),
			VALUE(TEXT(SM_1_Saturday__c)),
			0) +

		IF(
			NOT(ISBLANK(TEXT(SM_1_Sunday__c ))),
			VALUE(TEXT(SM_1_Sunday__c)),
			0)
	), 1
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Weekly Batch Count</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
