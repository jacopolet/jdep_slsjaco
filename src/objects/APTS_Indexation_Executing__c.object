    <validationRules>
        <fullName>APTS_EmptyIndexationnumber</fullName>
        <active>true</active>
        <description>If Indexation number is empty</description>
        <errorConditionFormula>ISBLANK(APTS_Indexation_Number__c)</errorConditionFormula>
        <errorDisplayField>APTS_Indexation_Number__c</errorDisplayField>
        <errorMessage>Please enter a defined indexation number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_ExecutionDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISBLANK(APTS_Execution_Date__c), APTS_Execution_Date__c &lt;= TODAY())</errorConditionFormula>
        <errorDisplayField>APTS_Execution_Date__c</errorDisplayField>
        <errorMessage>Please enter a Execution Date which is tomorrow or later</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractCountryValidation</fullName>
        <active>true</active>
        <description>Both Country and Contract cannot be defined for a record</description>
        <errorConditionFormula>AND(
      NOT(ISBLANK( APTS_Indexation_Contract__c )),
      NOT(ISBLANK( TEXT(APTS_Indexation_Country__c )))
 )</errorConditionFormula>
        <errorMessage>Please define only Country for Standard indexation or Contract for Non-Standard indexation</errorMessage>
    </validationRules>
