    </validationRules>
    <validationRules>
        <fullName>IndustryCodeRange_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, VALUE(Industry_Code_From__c) &gt; VALUE(Industry_Code_To__c))</errorConditionFormula>
        <errorMessage>Invalid IndustryCode Range</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Industry_Code_Number_Only_and_Length_4</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, OR(IF( NOT(ISBLANK(Industry_Code_From__c)), 
NOT(
AND(
LEN(Industry_Code_From__c) == 4, ISNUMBER(Industry_Code_From__c) )
), false),
IF( NOT(ISBLANK(Industry_Code_To__c)), 
NOT(
AND(
LEN(Industry_Code_To__c) == 4, ISNUMBER(Industry_Code_To__c) )
), false))
)</errorConditionFormula>
        <errorMessage>Industry Code should be 4 digits and only numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PostalCodeRange_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, VALUE(Postal_Code_From__c) &gt; VALUE(Postal_Code_To__c))</errorConditionFormula>
        <errorMessage>Invalid Postal Code Range</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postal_Code_Number_Only_and_Max_Length_5</fullName>
        <active>true</active>
        <description>Postal Code is only numbers and max 5 digits</description>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, OR(IF( NOT(ISBLANK(Postal_Code_From__c)), 
NOT( 
AND( 
LEN(Postal_Code_From__c) &lt;= 5, ISNUMBER(Postal_Code_From__c) ) 
), false),
IF( NOT(ISBLANK(Postal_Code_To__c)), 
NOT( 
AND( 
LEN(Postal_Code_To__c) &lt;= 5, ISNUMBER(Postal_Code_To__c) ) 
), false))
)</errorConditionFormula>
        <errorMessage>Postal code should be only numbers and max 5 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SeatsRange_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, VALUE(Seats_From__c) &gt; VALUE(Seats_To__c))</errorConditionFormula>
        <errorMessage>Invalid Seat Range</errorMessage>
    </validationRules>
    <validationRules>
