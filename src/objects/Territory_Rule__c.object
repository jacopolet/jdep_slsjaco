    <validationRules>
        <fullName>AreaCodeRange_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, VALUE(Area_Code_From__c ) &gt; VALUE(Area_Code_To__c ))</errorConditionFormula>
        <errorMessage>Invalid Area Code Range</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BedRange_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.ValidationRulesSettings__c.TerritoryRuleValidationRules__c, VALUE(Beds_From__c ) &gt; VALUE(Beds_To__c))</errorConditionFormula>
