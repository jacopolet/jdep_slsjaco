    <fields>
        <fullName>APTS_Incentive_Adjustment_Amount__c</fullName>
        <description>Field sets adjustment price field applicable to SAP. SAP will take data from this.</description>
        <externalId>false</externalId>
        <formula>IF( (ISPICKVAL(Apttus_Config2__LineItemId__r.Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c,&apos;XD09&apos;) ||ISPICKVAL(Apttus_Config2__LineItemId__r.Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c,&apos;XD01&apos;) ) ,Apttus_Config2__IncentiveAdjustmentAmount__c,
IF( ISPICKVAL(Apttus_Config2__LineItemId__r.Apttus_Config2__PriceType__c, &apos;Recurring&apos;), 
	IF(
		ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Monthly&apos;) , 
		(Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*1 , 
		IF( 
			ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Quarterly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*3, 
			IF( 
				ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Half Yearly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*6, IF( ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Yearly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*12,0)
			)
		)
	), 
	IF(APTS_Return_Adjustment_Price__c &gt;= 0.01, APTS_Return_Adjustment_Price__c, Apttus_Config2__IncentiveAdjustmentAmount__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Adjusted Price</inlineHelpText>
        <label>Incentive Adjustment Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>APTS_Incentive_Amount_Per_Billing_Period__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Monthly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*1 , IF( ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Quarterly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*3, IF( ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Half Yearly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*6, IF( ISPICKVAL( Apttus_Config2__LineItemId__r.Apttus_Config2__SellingFrequency__c , &apos;Yearly&apos;) , (Apttus_Config2__IncentiveAdjustmentAmount__c/ Apttus_Config2__LineItemId__r.Apttus_Config2__Term__c)*12,0))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Incentive Amount Per Billing Period</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
