<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Custom metadata to store batch queries</description>
    <fields>
        <fullName>APTS_API__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Checkbox to decide which api to use in pricing</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>API</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>APTS_Batch_Name__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Batch Name</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Control__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Field used to execute validations in batch code</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Control</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>APTS_Effective_Date__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Effective Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>APTS_Query_String__c</fullName>
        <description>Field to store queries</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Query String</label>
        <length>131072</length>
        <type>LongTextArea</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>APTS_Times_Reprice__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Times to Reprice</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Triggers__c</fullName>
        <description>Comma separated list of triggers to be deactivated</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Triggers</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Batch query</label>
    <pluralLabel>Batch queries</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
