        <externalId>false</externalId>
        <formula>Case__r.AccountLocation__r.Name + &apos;|&apos; +
Case__r.PhysicalAsset__r.ProductDescription__c + &apos;|&apos; +
Case__r.PhysicalAsset__r.SerialNumber__c + &apos;|&apos; +
Case__r.PhysicalAsset__r.Building__c + &apos;|&apos; +
Case__r.PhysicalAsset__r.Floor__c + &apos;|&apos; +
Case__r.PhysicalAsset__r.Area__c + &apos;|&apos; +
Case__r.PhysicalAsset__r.CustomerReferenceNumber__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Parsing Text for DevOps Operations.</inlineHelpText>
