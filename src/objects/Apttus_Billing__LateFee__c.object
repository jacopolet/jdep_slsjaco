    <fields>
        <fullName>Apttus_Billing__LateFeeType__c</fullName>
        <deprecated>false</deprecated>
        <description>Late Fee Type field</description>
        <externalId>false</externalId>
        <inlineHelpText>Use this field to define the type of the late fee. The value can be an amount or percentage and is based on the Late Fee Type definition.</inlineHelpText>
        <label>Late Fee Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Percentage</fullName>
                    <default>true</default>
                    <label>Percentage</label>
                </value>
                <value>
                    <fullName>Amount</fullName>
                    <default>false</default>
                    <label>Amount</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Apttus_Billing__LateFeeValue__c</fullName>
        <deprecated>false</deprecated>
        <description>Late Fee Value Field</description>
        <externalId>false</externalId>
        <inlineHelpText>Use this field to define the value of the late fee. The value could be an amount or percentage and is based on the Late Fee Type definition.</inlineHelpText>
        <label>Late Fee Value</label>
        <precision>18</precision>
        <required>true</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Late Fee</label>
    <nameField>
        <label>Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Late Fees</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Apttus_Billing__LateFeeValuePercentageValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Apttus_Billing__LateFeeType__c , &apos;Percentage&apos;) ,  OR( Apttus_Billing__LateFeeValue__c &lt; 0, Apttus_Billing__LateFeeValue__c &gt; 100) )</errorConditionFormula>
        <errorDisplayField>Apttus_Billing__LateFeeValue__c</errorDisplayField>
        <errorMessage>The specified Late Fee percentage must be greater than zero and less than or equal to 100.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
