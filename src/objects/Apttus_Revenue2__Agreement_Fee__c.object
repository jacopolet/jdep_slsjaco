    <validationRules>
        <fullName>Apttus_Revenue2__Fee_Type_is_required</fullName>
        <active>true</active>
        <description>Fee Type is required</description>
        <errorConditionFormula>ISPICKVAL( Apttus_Revenue2__Fee_Type__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Fee_Type__c</errorDisplayField>
        <errorMessage>Fee Type must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Fee_requires_revenue_recognition_rule</fullName>
        <active>true</active>
        <description>Fee requires a revenue recognition rule to be specified</description>
        <errorConditionFormula>ISPICKVAL( Apttus_Revenue2__RevRec_Rule__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__RevRec_Rule__c</errorDisplayField>
        <errorMessage>Please specify a Revenue recognition rule for the fee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Fixed_or_Variable_is_required</fullName>
        <active>false</active>
        <description>Fee has to be specified as either a Fixed or Variable Fee</description>
        <errorConditionFormula>ISPICKVAL( Apttus_Revenue2__Fixed_Variable__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Fixed_Variable__c</errorDisplayField>
        <errorMessage>Please specify Fee as either Fixed or Variable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Flat_Fee_required_for_fee_type_Flat_Fee</fullName>
        <active>true</active>
        <description>Flat Fee must be entered if Fee Type is Flat Fee</description>
        <errorConditionFormula>AND(ISPICKVAL(Apttus_Revenue2__Fee_Type__c,&quot;Flat Fee&quot;), ISNULL( Apttus_Revenue2__Flat_Fee__c ))</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Flat_Fee__c</errorDisplayField>
        <errorMessage>Flat Fee is required if Fee Type is Flat Fee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Frequency_required_for_Recurring_Fees</fullName>
        <active>true</active>
        <description>Frequency is required for a recurring fee</description>
        <errorConditionFormula>AND(ISPICKVAL( Apttus_Revenue2__One_Time_Recurring__c, &quot;Recurring&quot;), ISPICKVAL( Apttus_Revenue2__Frequency__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Frequency__c</errorDisplayField>
        <errorMessage>Frequency must be entered for a Recurring Fee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Percent_of_Revenue_reqd_for_the_Fee_Type</fullName>
        <active>true</active>
        <description>If Fee type is % of Revenue or Revenue Share, then Percent of Revenue must be entered.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Apttus_Revenue2__Fee_Type__c,&quot;% of Revenue&quot;), ISPICKVAL(Apttus_Revenue2__Fee_Type__c,&quot;Revenue Share&quot;)), ISNULL(  Apttus_Revenue2__Percent_of_Revenue__c ))</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Percent_of_Revenue__c</errorDisplayField>
        <errorMessage>Percent of Revenue is required if Fee Type is % of Revenue or Revenue Share</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus_Revenue2__Unit_Price_for_Fee_Type_equals_PerUnit</fullName>
        <active>true</active>
        <description>If Fee type is Per Unit or Per User or Revenue Share, then a Unit Price must be entered.</description>
        <errorConditionFormula>AND( OR(ISPICKVAL(Apttus_Revenue2__Fee_Type__c ,&quot;Per Unit&quot;), ISPICKVAL(Apttus_Revenue2__Fee_Type__c,&quot;Per User&quot;), ISPICKVAL(Apttus_Revenue2__Fee_Type__c,&quot;Revenue Share&quot;)), ISNULL(Apttus_Revenue2__Unit_Price__c ))</errorConditionFormula>
        <errorDisplayField>Apttus_Revenue2__Unit_Price__c</errorDisplayField>
        <errorMessage>Unit Price is required if Fee Type is Per Unit or Per User or Revenue Share</errorMessage>
    </validationRules>
