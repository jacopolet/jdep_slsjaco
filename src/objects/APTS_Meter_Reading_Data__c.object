    <validationRules>
        <fullName>TS_AllowedCorrectiveCounterReading</fullName>
        <active>false</active>
        <description>You are not allowed to create a correct counter reading job.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.MeterReadingValidationRule__c &amp;&amp;
ISPICKVAL( APTS_Status__c , &quot;Completed&quot;) &amp;&amp;
ISCHANGED( APTS_Status__c ) &amp;&amp;
ISCHANGED( APTS_Reason_code_SFDC__c ) &amp;&amp;
ISPICKVAL(APTS_Reason_code_SFDC__c, &quot;Work Order Created&quot;) &amp;&amp;
NOT( $Permission.TS_S22 )</errorConditionFormula>
        <errorMessage>You are not allowed to create a correct counter reading job.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CantCreateCRFUPInactive</fullName>
        <active>true</active>
        <description>You cannot create Counter Reading FUP if machine is inactive.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.MeterReadingValidationRule__c &amp;&amp; 
ISPICKVAL( APTS_Status__c , &quot;Completed&quot;) &amp;&amp; 
ISCHANGED( APTS_Status__c ) &amp;&amp; 
ISPICKVAL(APTS_Corrective_Action__c, &quot;2&quot;) &amp;&amp; 
NOT(ISPICKVAL( PhysicalAsset__r.AssetStatus__c, &apos;Activated&apos;))</errorConditionFormula>
        <errorMessage>You cannot create Counter Reading FUP if machine is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CantCreateCRFUPNotRequired</fullName>
        <active>true</active>
        <description>You cannot create Counter Reading FUP if counter is not required in the machine.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.MeterReadingValidationRule__c &amp;&amp; 
ISPICKVAL( APTS_Status__c , &quot;Completed&quot;) &amp;&amp; 
ISCHANGED( APTS_Status__c ) &amp;&amp; 
ISPICKVAL(APTS_Corrective_Action__c, &quot;2&quot;) &amp;&amp; 
ISPICKVAL(PhysicalAsset__r.CounterReading__c, &apos;No&apos;)</errorConditionFormula>
        <errorMessage>You cannot create Counter Reading FUP if counter is not required in the machine.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CorrectiveCounterReadingPreConditions</fullName>
        <active>false</active>
        <description>You can only create corrective jobs if the machine is related to this record and requires counters and is also not connected.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.MeterReadingValidationRule__c &amp;&amp;
ISPICKVAL( APTS_Status__c , &quot;Completed&quot;) &amp;&amp;
ISCHANGED( APTS_Status__c ) &amp;&amp;
ISCHANGED( APTS_Reason_code_SFDC__c ) &amp;&amp;
ISPICKVAL(APTS_Reason_code_SFDC__c, &quot;Work Order Created&quot;) &amp;&amp;
OR( 
ISBLANK( PhysicalAsset__c ),
ISPICKVAL( PhysicalAsset__r.Connected__c , &quot;Yes&quot;), 
ISPICKVAL( PhysicalAsset__r.CounterReading__c, &quot;No&quot;))</errorConditionFormula>
        <errorMessage>You can only create corrective jobs if the machine is related to this record and requires counters and is also not connected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_UnabletoModifiyCompletedMeterReadings</fullName>
        <active>true</active>
        <description>You are not allowed to adjust completed meter reading data records.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.MeterReadingValidationRule__c = TRUE &amp;&amp;
ISPICKVAL( APTS_Status__c, &quot;Completed&quot;) &amp;&amp; 
NOT(ISCHANGED(APTS_Status__c))</errorConditionFormula>
        <errorMessage>You are not allowed to adjust completed meter reading data records.</errorMessage>
    </validationRules>
