    <validationRules>
        <fullName>CreditMemoRequestMandatoryFields</fullName>
        <active>true</active>
        <description>When creating a Credit Memo Request, at least one of the following three fields need to be filled-in to accept:
•	Invoice
•	Case   
•	Description</description>
        <errorConditionFormula>AND(Case__c == null, Apttus_Billing__InvoiceID__c == null, Apttus_Billing__Description__c ==null)</errorConditionFormula>
        <errorMessage>Credit Memo Request requires at least one of three fields(invoice, Case,Descrption) need to be filled-in to accept.  Please fill the missing field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_CreditMemoOnClosedCancelledCase</fullName>
        <active>true</active>
        <description>You cannot create a credit memo for a closed or canceled case.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.CaseValidationRules__c &amp;&amp;
NOT(ISBLANK( Case__c )) &amp;&amp;
OR(
ISPICKVAL(Case__r.Status, &quot;Closed&quot;),
ISPICKVAL(Case__r.Status, &quot;Canceled&quot;))</errorConditionFormula>
        <errorMessage>You cannot create a credit memo for a closed or canceled case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_PreventNonOwnersCreatingCreditMemos</fullName>
        <active>false</active>
        <description>Only the owner of a case can create a credit memo if a credit memo is linked to a case.</description>
        <errorConditionFormula>$Setup.ValidationRulesSettings__c.CaseValidationRules__c &amp;&amp;
NOT(ISBLANK( Case__c )) &amp;&amp;
Case__r.LastModifiedById &lt;&gt;  Case__r.OwnerId</errorConditionFormula>
        <errorMessage>Only the owner of a case can create a credit memo if a credit memo is linked to a case.</errorMessage>
    </validationRules>
