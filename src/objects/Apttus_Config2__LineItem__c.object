        <externalId>false</externalId>
        <formula>IF( 
  APTS_ATP_Cleared__c = TRUE,
  
  IF( 
     AND(Apttus_Config2__Quantity__c &lt;= APTS_Expected_Delivery_Quantity__c,  
     APTS_Requested_Delivery_Date__c = APTS_Expected_Delivery_Date__c), 
     
     IMAGE(&quot;/resource/1498724254000/GreenTrafficLight&quot;, &quot;OK&quot;), 
     
     IF(
        AND(Apttus_Config2__Quantity__c &gt; APTS_Expected_Delivery_Quantity__c, 
        APTS_Requested_Delivery_Date__c = APTS_Expected_Delivery_Date__c), 

        IMAGE(&quot;/resource/1498724234000/YellowTrafficLight&quot;, &quot;OK&quot;), 

        IMAGE(&quot;/resource/1498724205000/RedTrafficLight&quot;, &quot;OK&quot;) 
       )
    ), 

  IMAGE(&quot;/resource/1498724270000/GreyTrafficLight&quot;, &quot;OK&quot;) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
