    </fields>
    <fields>
        <fullName>APTS_Duration1__c</fullName>
        <description>Selling term: difference between start date and end date.</description>
        <externalId>false</externalId>
        <label>Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Duration__c</fullName>
        <externalId>false</externalId>
        <formula>(YEAR(APTS_End_date__c) - YEAR(APTS_Start_date__c) - IF(MONTH(APTS_End_date__c) &lt; MONTH(APTS_Start_date__c),1,0))*12

+

(MONTH(APTS_End_date__c) - MONTH(APTS_Start_date__c) + IF(DAY(APTS_End_date__c) &gt; DAY(APTS_Start_date__c),12,0))

+

IF(DAY(APTS_End_date__c) &gt; DAY(APTS_Start_date__c),DAY(APTS_End_date__c) - DAY(APTS_Start_date__c),DAY(APTS_Start_date__c) - DAY(APTS_End_date__c))/30</formula>
        <label>Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_End_date__c</fullName>
        <externalId>false</externalId>
        <label>End date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>APTS_Isinactive__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Inactive Historical Price change record</description>
        <externalId>false</externalId>
        <label>Isinactive</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>APTS_Net_Price__c</fullName>
        <externalId>false</externalId>
        <label>Net Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>5</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>APTS_Price__c</fullName>
        <externalId>false</externalId>
        <formula>(APTS_Net_Price__c / APTS_Selling_term__c) *  APTS_Duration1__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
