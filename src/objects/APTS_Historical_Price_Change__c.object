    <fields>
        <fullName>APTS_AssetLineItem__c</fullName>
        <externalId>false</externalId>
        <label>Asset Line Item</label>
        <referenceTo>Apttus_Config2__AssetLineItem__c</referenceTo>
        <relationshipLabel>Historical Price Changes</relationshipLabel>
        <relationshipName>Historical_Price_Changes</relationshipName>
