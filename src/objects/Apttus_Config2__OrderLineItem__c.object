        <externalId>false</externalId>
        <formula>OR(ISPICKVAL(Apttus_Config2__LineType__c, &apos;Misc&apos;),ISPICKVAL(Apttus_Config2__LineType__c, &apos;Option&apos;), AND(ISPICKVAL(Apttus_Config2__LineType__c, &apos;Product/Service&apos;), Apttus_Config2__HasOptions__c = FALSE))</formula>
