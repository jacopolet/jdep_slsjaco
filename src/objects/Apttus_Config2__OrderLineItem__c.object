    <fields>
        <fullName>APTS_ATP_Check_Status__c</fullName>
        <description>This is the ATP Check Status</description>
        <externalId>false</externalId>
        <formula>IF( 
APTS_ATP_Cleared__c = TRUE, 
IF( 
OR(ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c, &apos;Operating Order&apos;), 
ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c, &apos;Route Sales Order&apos;), 
ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c, &apos;Standard Order&apos;)), 
IF( 
AND(Apttus_Config2__Quantity__c &lt;= APTS_Expected_Delivery_Quantity__c, 
APTS_Requested_Delivery_Date__c = APTS_Expected_Delivery_Date__c), 
IMAGE(&quot;/resource/1498724254000/GreenTrafficLight&quot;, &quot;OK&quot;), 
IF(AND(Apttus_Config2__Quantity__c &gt; APTS_Expected_Delivery_Quantity__c, 
APTS_Requested_Delivery_Date__c = APTS_Expected_Delivery_Date__c), 
IMAGE(&quot;/resource/1498724234000/YellowTrafficLight&quot;, &quot;OK&quot;), 
IMAGE(&quot;/resource/1498724205000/RedTrafficLight&quot;, &quot;OK&quot;)) 
), 
IF( 
APTS_Requested_Installation_Date__c &gt;= APTS_Expected_Delivery_Date__c, 
IMAGE(&quot;/resource/1498724254000/GreenTrafficLight&quot;, &quot;OK&quot;), 
IMAGE(&quot;/resource/1498724205000/RedTrafficLight&quot;, &quot;OK&quot;) 
) 
), 
IMAGE(&quot;/resource/1498724270000/GreyTrafficLight&quot;, &quot;OK&quot;) 
)</formula>
        <label>ATP Check Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
