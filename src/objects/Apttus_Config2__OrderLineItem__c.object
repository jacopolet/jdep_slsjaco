				ISPICKVAL(APTS_Type_of_Contract__c,&quot;Rent&quot;),
				ISPICKVAL(APTS_Type_of_Contract__c,&quot;RentBuy&quot;),
				ISPICKVAL(APTS_Type_of_Contract__c,&quot;Free on Loan&quot;),
