    <fields>
        <fullName>APTS_Is_Primary_L1_Line__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
	ISPICKVAL(Apttus_Config2__LineType__c,&quot;Product/Service&quot;), 
	OR( 
		AND(
			ISPICKVAL(APTS_Type_of_Contract__c,&quot;Sales&quot;), 
			ISPICKVAL(Apttus_Config2__ChargeType__c,&quot;Sales Price&quot;),
			OR(
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XA13&quot;),
                                ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XD01&quot;),
ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XD02&quot;),
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XB13&quot;),
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XA14&quot;)
				)
			),
		AND(
			ISPICKVAL(APTS_Type_of_Contract__c,&quot;Trial&quot;), 
			ISPICKVAL(Apttus_Config2__ChargeType__c,&quot;Sales Price&quot;),
			OR(
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XA13&quot;),
                                ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XD01&quot;),
ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XD02&quot;),
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XB13&quot;),
				ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XA14&quot;),
