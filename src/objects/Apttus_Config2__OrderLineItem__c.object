                                ISPICKVAL(Apttus_Config2__OrderId__r.APTS_SAP_OrderType__c, &quot;XC09&quot;)
				)
			),
		AND(
			ISPICKVAL(APTS_Type_of_Contract__c,&quot;Subscription&quot;), 
			ISPICKVAL(Apttus_Config2__ChargeType__c,&quot;Sales Price&quot;),
			OR(
