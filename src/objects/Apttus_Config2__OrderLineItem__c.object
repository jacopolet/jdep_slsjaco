        <externalId>false</externalId>
        <formula>if(OR(ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Machine Order&apos;),ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Admin Order&apos;),ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Credit Memo&apos;),ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Debit Memo&apos;)),
if(OR(ISPICKVAL(APTS_Refurbished_or_New__c,&apos;U&apos;),ISPICKVAL(APTS_Refurbished_or_New__c,&apos;N&apos;)), if(Apttus_Config2__PreviousVersion__c != null, TEXT(APTS_Refurbished_or_New__c)+RIGHT(Apttus_Config2__OriginalLineNumber__c,5), TEXT(APTS_Refurbished_or_New__c)+RIGHT(Name,5)), &apos;&apos;), 
if(OR(ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Standard Order&apos;), ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Route Sales Order&apos;), ISPICKVAL(Apttus_Config2__OrderId__r.APTS_Order_Type__c,&apos;Operating Order&apos;)),
if(Apttus_Config2__PreviousVersion__c != null, RIGHT(Apttus_Config2__OriginalLineNumber__c,6), RIGHT(Name,6)), &apos;&apos;))</formula>
