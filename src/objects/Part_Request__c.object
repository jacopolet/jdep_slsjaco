    <validationRules>
        <fullName>TS_CannotPutStatusBacktoOpen</fullName>
        <active>true</active>
        <description>You cannot reopen a completed request. Please create a new one.</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.PartRequestValidationRules__c,ISPICKVAL(PRIORVALUE(Status__c),&quot;Completed&quot;) &amp;&amp; ISPICKVAL(Status__c,&quot;Open&quot;),false)</errorConditionFormula>
        <errorMessage>You cannot reopen a completed request. Please create a new one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_NumofPartsGreaterthan1</fullName>
        <active>true</active>
        <description>The Number of Spare Parts field should be greater than 0 before completing the part request.</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.PartRequestValidationRules__c, ISPICKVAL( Status__c , &quot;Completed&quot;) &amp;&amp; Number_of_Parts__c  &lt; 1,
FALSE)</errorConditionFormula>
        <errorMessage>Please add parts in your order before finalizing the request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_OrderCompletedAlready</fullName>
        <active>true</active>
        <description>Order is already completed.</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.PartRequestValidationRules__c,ISPICKVAL(PRIORVALUE(Status__c),&quot;Completed&quot;) &amp;&amp; ISPICKVAL(Status__c,&quot;Completed&quot;) &amp;&amp; (PRIORVALUE( SAP_Purchase_Order__c ) = SAP_Purchase_Order__c  ),false)</errorConditionFormula>
        <errorMessage>Order is already completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_PopulateDetails</fullName>
        <active>true</active>
        <description>You can only create part request if SAP Plant, SAP Storage Location, Company Code and Sales Organization are populated.</description>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.PartRequestValidationRules__c,
OR( ISBLANK( Resource__r.SAPPlant__c ),
 ISBLANK( Resource__r.SAPStorageLocation__c ),
 ISBLANK( Resource__r.CompanyCode__c ),
 ISBLANK( Resource__r.SalesOrg__c )),
FALSE)</errorConditionFormula>
        <errorMessage>You can only create part request if the following fields in the resource are populated: SAP Plant, SAP Storage Location &amp; Company Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_UnableToCancelCompletedRequest</fullName>
        <active>true</active>
        <errorConditionFormula>IF( $Setup.ValidationRulesSettings__c.PartRequestValidationRules__c, 
	ISCHANGED(Status__c) &amp;&amp; 
	ISPICKVAL(PRIORVALUE(Status__c),&quot;Completed&quot;) &amp;&amp;
    ISPICKVAL(Status__c, &quot;Canceled&quot;),
	FALSE)</errorConditionFormula>
        <errorMessage>Fulfilled requests cannot be canceled.</errorMessage>
    </validationRules>
