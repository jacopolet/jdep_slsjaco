        <active>true</active>
        <description>Enter either a unit rate or a tier price</description>
        <errorConditionFormula>AND(ISNULL( Apttus_Revenue2__Tier_Price__c ), ISNULL( Apttus_Revenue2__Tier_Rate__c ))</errorConditionFormula>
