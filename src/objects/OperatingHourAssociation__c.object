    <validationRules>
        <fullName>TS_RequiredValues</fullName>
        <active>true</active>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.OperatingHourAssociationValidationRule__c,
	IF( 
		OR(
			CustomServiceHours__c = FALSE &amp;&amp; 
				OR(	TEXT(StartofBusinessDay__c) = null, 
					TEXT(EndofBusinessDay__c) = null, 
					TEXT(MondayException__c) = null, 
					TEXT(TuesdayException__c) = null, 
					TEXT(WednesdayException__c) = null, 
					TEXT(ThursdayException__c) = null, 
					TEXT(FridayException__c) = null),
			CustomServiceHours__c &amp;&amp; CustomServiceHoursDescription__c = null
			),
		TRUE,
		FALSE),
	FALSE)</errorConditionFormula>
        <errorMessage>Please provide start &amp; end of business hours and close time per day. If custom service hour is set to true, please provide a custom service hour description.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TS_StartEndOfBusinessDaySequence</fullName>
        <active>true</active>
        <errorConditionFormula>IF($Setup.ValidationRulesSettings__c.OperatingHourAssociationValidationRule__c,
	IF(
		AND(ISNUMBER(MID(TEXT(StartofBusinessDay__c),1,2)),
			ISNUMBER(MID(TEXT(EndofBusinessDay__c),1,2))
			),
			IF(VALUE(MID(TEXT(StartofBusinessDay__c),1,2)) &gt;= VALUE(MID(TEXT(EndofBusinessDay__c),1,2)), TRUE, FALSE),
			FALSE
	),	 
FALSE)</errorConditionFormula>
        <errorMessage>Start of Business Day cannot be equal or later than End of Business Day</errorMessage>
    </validationRules>
